{
    "name": "omga-auto-commit",
    "displayName": "Omga Auto Commit",
    "description": "A VS Code extension for automatically committing and pushing code changes.",
    "version": "0.1.5",
    "publisher": "PouriaHosseini",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "SCM Providers",
        "Other"
    ],
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/isPoori/auto-commit"
    },
    "bugs": {
        "url": "https://github.com/isPoori/auto-commit/issues"
    },
    "activationEvents": [
        "onCommand:extension.enableAutoCommit",
        "onCommand:extension.disableAutoCommit"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.enableAutoCommit",
                "title": "Enable Auto Commit"
            },
            {
                "command": "extension.disableAutoCommit",
                "title": "Disable Auto Commit"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js",
        "package": "vsce package",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/vscode": "^1.60.0",
        "@types/glob": "^7.1.3",
        "@types/node": "14.x",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "vscode-test": "^1.5.0"
    }
}
